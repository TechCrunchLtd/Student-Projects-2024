void setAddressIndex( void )
{
    if(millis() - addr_millis < ADDRESS_RATE_mS) { return;}
    addr_millis = millis();
    
    uint8_t limit = addresses.length();
    start_index = (start_index < limit) ? start_index+1 : 0;
    stop_index = (stop_index < limit) ? stop_index+1 : 0;

    if(stop_index > start_index) { 
      subAdd = addresses.substring(start_index, stop_index);
    }
    else{
      subAdd = addresses.substring(start_index) + addresses.substring(0, stop_index);
    }
}

void drawCross( void )
{
  
  int y = 0, a = 117, b = 55, c = 6;
  while (y++ < 7)
  {
    display.drawPixel(a, b, OLED_COLOR);
    display.drawPixel(a++, c + b++, OLED_COLOR);
    c -= 2;
  }
}

void drawTick( void )
{
 
  int y = 0, a = 123, b = 56;
  while (y++ < 7)
  {
    if (y < 5)
      display.drawPixel(a--, b++, OLED_COLOR);
    else
      display.drawPixel(a--, b--, OLED_COLOR);
  }
}

bool light_displayInit( void )
{
    stop_index = ADDRESS_LENGTH;
    #ifndef USING_NODEMCU
        Wire.pins(SDA_PIN, SCL_PIN); // use this for ESP-01 only
        Wire.begin(SDA_PIN, SCL_PIN); // sda, scl
    #endif
    
    #ifdef SSD1306
        display.begin(SSD1306_SWITCHCAPVCC, DISPLAY_ADDRESS);
    #else 
        display.begin(DISPLAY_ADDRESS, true);
        display.setRotation(1);
    #endif

    #ifdef USING_NODEMCU
        Serial.println("display init");
    #endif
    
    for(int i=0; i<LOGO_SIZE; i++){
        display.clearDisplay();
        display.drawBitmap(0, 0, techCrunchLogo[i], 128, 64, OLED_COLOR);
        display.display();
        delay(100);
    }
    // display.clearDisplay();
    // display.drawBitmap(0, 0, logo_white, 128, 64, OLED_COLOR);
    // display.display();
    delay(1000);

    for(int i=LOGO_SIZE-1; i>0; i--){
        display.clearDisplay();
        display.drawBitmap(0, 0, techCrunchLogo[i], 128, 64, OLED_COLOR);
        display.display();
        delay(50);
    }
    
    display.clearDisplay();

    uint8_t len = (display.height() / 2) - 6;
    uint8_t wid = display.width() / 4;
    // draw rectangles
    for (int16_t i = 0; i < len; i += 2)
    {
        display.drawRect(i, i, display.width() - 2 * i, display.height() - 2 * i, OLED_COLOR);
        display.display(); // Update screen with each newly-drawn rectangle
        delay(100);
    }
    display.setTextSize(1);
    display.setTextColor(OLED_COLOR);
    display.setCursor(wid-3, len+2);
    display.println("TECH CRUNNCH");
    display.display();
    delay(3000);

    display.clearDisplay();
    display.display();
    
    return true;
}

void light_displayUpdate( void )
{
    display.clearDisplay();

    if(millis() - disp_millis < SCREEN_INTERVAL * 1000)
    {
        #ifdef USING_NODEMCU
            Serial.println(light_params.whatsapp_number);
            Serial.println(light_params.wifi_name);
            // Serial.println(light_params.current_date);
            // Serial.println(light_params.current_time);
            Serial.println(light_params.server_address);
            // Serial.println(light_params.next_alarm);
            Serial.println(light_params.server_running);
            Serial.println(light_params.light_activated);
            Serial.println(light_params.light_alarm_on);
            Serial.println(light_params.update_interval);
        #endif
        display.drawRect(0, 0, display.width(), display.height() - 13, OLED_COLOR);
        display.setTextSize(1);
        display.setTextColor(OLED_COLOR);

        display.setCursor(3, 3);
        display.print(light_params.whatsapp_number); //("256782912385");

        display.setCursor(3, 17);
        // display.print(light_params.current_date); //("WEDNESDAY 22 NOV 2023");
        display.print(light_params.wifi_name); //(" TECH CRUNCHERS SCH ")

        display.setCursor(3, 28);
        if(light_params.server_running)
            display.print(light_params.server_address); //display.print(WiFi.softAPIP())
		else
			display.print("Not Connected");

        display.setCursor(3, 40);
        if(light_params.light_activated)
            display.print("LIGHT IS ON");
        else
            display.print("LIGHT IS OFF");
        
        display.drawRect(113, 53, 15, 11, OLED_COLOR); // display wifi status
        (light_params.server_running) ? drawTick() : drawCross();

        setAddressIndex();
        display.setCursor(1, 55);
        display.print(subAdd);

    }
    else
    {
        start_index = 0;
        stop_index = ADDRESS_LENGTH;
    }

    display.display();
}

void light_displayRefresh( void )
{
    disp_millis = millis();
}

void light_displayError( char msg[] )
{
    #ifdef USING_NODEMCU
        Serial.println(msg);
    #endif
    
    display.clearDisplay();

    display.setTextSize(4);
    display.setTextColor(OLED_COLOR);
    display.print(msg);

    display.display();
}

